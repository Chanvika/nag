pipeline{
    agent any
    stages{
        stage('checkout'){
            steps {
                checkout scmGit(branches: [[name: '*/dev']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Venna12/taxi-booking.git']])
            }
        }
        stage('build'){
            steps {
                sh 'mvn package'
            }
        }
        stage('deploy'){
            steps {
                deploy adapters: [tomcat9(credentialsId: 'e0e2180b-b833-4294-87a5-be2d6d5efe89', path: '', url: 'http://16.16.70.212:8080/')], contextPath: null, war: '**/*.war'
            }
        }
    }
}
